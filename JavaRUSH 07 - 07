package com.javarush.task.task07.task0707;

import java.util.ArrayList;

/* 
Что за список такой?
1. Создай список строк.
2. Добавь в него 5 различных строк.
3. Выведи его размер на экран.
4. Используя цикл выведи его содержимое на экран, каждое значение с новой строки.
*/

public class Solution {
    public static void main(String[] args) {
        ArrayList<String> list = new ArrayList<String>();   //Создай список строк.
        list.add("Строка №1");  //Добавь в него 5 различных строк.
        list.add("Строка №2");  //Добавь в него 5 различных строк.
        list.add("Строка №3");  //Добавь в него 5 различных строк.
        list.add("Строка №4");  //Добавь в него 5 различных строк.
        list.add("Строка №5");  //Добавь в него 5 различных строк.
        System.out.println(list.size()); //Выведи его размер на экран.
        for (int i = 0; i < list.size(); i++)
        {
            System.out.println(list.get(i));    //Используя цикл выведи его содержимое на экран, каждое значение с новой строки.
        }
    }
}
